{
  "build": {
    "env": {
      "NEXT_BUILD": "1"
    }
  },
  "outputDirectory": "out",
  "framework": "nextjs",
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "/$1/",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}